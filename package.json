{
  "name": "eslint-config-mdh-typescript-react",
  "version": "1.1.0",
  "description": "eslint config for TypeScript React",
  "keywords": [
    "eslint",
    "react",
    "TypeScript",
    "lint",
    "config",
    "mdh",
    "frontend"
  ],
  "main": "index.js",
  "repository": "git@github.com:mydatahack/eslint-config-mdh-typescript-react.git",
  "author": "mdh",
  "license": "MIT",
